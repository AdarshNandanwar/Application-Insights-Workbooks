    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "SigninLogs\r\n| extend DeviceId = tostring(DeviceDetail.deviceId)\r\n| where AppId == \"38aa3b87-a06d-4817-b275-7a316988d93b\" //Windows Sign-In\r\n| where ResultType == 50155 //Device authentication failed for this user.\r\n| project TimeGenerated, Id, CorrelationId, UserPrincipalName\r\n| summarize TotalRequests=count() by UserPrincipalName\r\n| sort by TotalRequests desc\r\n",
        "size": 1,
        "showAnalytics": true,
        "title": "Users with device authentication errors",
